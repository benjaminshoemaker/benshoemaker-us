{
  "commands": {
    "test": "",
    "lint": "",
    "typecheck": "npx astro check",
    "build": "npm run build",
    "coverage": ""
  },
  "devServer": {
    "command": "npm run dev",
    "url": "http://localhost:4321",
    "startupSeconds": 5
  },
  "auth": {
    "strategy": "none",
    "loginRoute": "/login",
    "credentials": {
      "usernameVar": "TEST_USER_EMAIL",
      "passwordVar": "TEST_USER_PASSWORD"
    },
    "storageState": ".claude/verification/auth-state.json"
  },
  "browser": {
    "tool": "auto",
    "headless": true,
    "timeout": 30000,
    "navigationTimeout": 60000
  }
}
